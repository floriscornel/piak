{
    "name": "{{ .PackageName }}",
    "description": "{{ .Description }}",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "{{ .JSONNamespace }}\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{ .JSONNamespace }}\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "osteel/openapi-httpfoundation-testing": "^0.11"
    }
} 